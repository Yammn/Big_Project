<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="700.0" styleClass="test_background" stylesheets="@../../../style/option.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dictionaryapp.GoogleController">
   <children>
      <TextArea fx:id="textFrom" layoutX="30.0" layoutY="90.0" prefHeight="215.0" prefWidth="640.0" stylesheets="@../../../style/style.css" wrapText="true">
         <font>
            <Font name="Arial" size="13.0" />
         </font></TextArea>
      <TextArea fx:id="textTo" layoutX="30.0" layoutY="432.0" prefHeight="215.0" prefWidth="640.0" stylesheets="@../../../style/style.css" wrapText="true">
         <font>
            <Font name="Arial" size="13.0" />
         </font></TextArea>
      <ComboBox fx:id="langFrom" layoutX="30.0" layoutY="62.0" prefWidth="150.0" promptText="Language From" styleClass="combo-box-base" stylesheets="@../../../style/style.css" />
      <ComboBox fx:id="langTo" layoutX="30.0" layoutY="404.0" prefWidth="150.0" promptText="Language To" styleClass="combo-box-base" stylesheets="@../../../style/style.css" />
      <ImageView fitHeight="35.0" fitWidth="49.0" layoutX="37.0" layoutY="265.0" onMouseClicked="#spelling1" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/audio.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="35.0" fitWidth="49.0" layoutX="37.0" layoutY="607.0" onMouseClicked="#spelling2" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/audio.png" />
         </image>
      </ImageView>
      <JFXButton layoutX="468.0" layoutY="339.0" onAction="#APISearch" prefHeight="52.0" prefWidth="162.0" styleClass="translate" stylesheets="@../../../style/style.css" text="Translate">
         <graphic>
            <ImageView fitHeight="43.0" fitWidth="49.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/translate-submit.png" />
               </image>
            </ImageView>
         </graphic>
         <font>
            <Font name="Arial" size="20.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="chooseFile" layoutX="588.0" layoutY="50.0" onAction="#setChooseFile" prefHeight="27.0" prefWidth="77.0" styleClass="chooseFile" stylesheets="@../../../style/style.css" text="Choose File">
         <font>
            <Font name="Arial" size="11.0" />
         </font></JFXButton>
      <Label fx:id="LabelLangFrom" layoutX="65.0" layoutY="349.0" prefHeight="35.0" prefWidth="169.0" styleClass="tof" stylesheets="@../../../style/button.css" text="Language From">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font>
      </Label>
      <Label fx:id="LabelLangTo" layoutX="288.0" layoutY="348.0" prefHeight="35.0" prefWidth="169.0" styleClass="tof" stylesheets="@../../../style/button.css" text="Language To">
         <font>
            <Font name="Century Gothic" size="13.0" />
         </font></Label>
      <ImageView fitHeight="35.0" fitWidth="35.0" layoutX="244.0" layoutY="349.0" onMouseClicked="#transfer" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/transfer.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
