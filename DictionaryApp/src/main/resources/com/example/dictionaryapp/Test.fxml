      <CheckBox fx:id="choice" layoutX="66.0" layoutY="65.0" mnemonicParsing="false" text="CheckBox" />
      <Button layoutX="609.0" layoutY="45.0" mnemonicParsing="false" onAction="#remove" text="Delete" />
